    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "rimraf": "^6.0.1",
    "typescript": "~5.6.3"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn run build",
    "clean": "rimraf lib",
    "build": "tsc",
    "lint": "eslint -c ../../../.eslintrc.json --ext .ts,.tsx ./src",
    "lint:fix": "eslint -c ../../../.eslintrc.json --fix --ext .ts,.tsx ./src",
    "watch": "tsc -w"
