{
  "private": true,
  "name": "@example/model-hub",
  "description": "Example Theia extension demonstrating use of the Model Hub and Model Services.",
  "license": "(EPL-2.0 OR MIT)",
  "repository": "https://github.com/eclipse-emfcloud/modelhub",
  "keywords": [
    "theia-extension"
  ],
  "version": "0.0.0",
  "files": [
    "lib",
    "src",
    "LICENSE"
  ],
  "dependencies": {
    "@theia/core": "*",
