{
  "version": "independent",
  "npmClient": "yarn",
  "command": {
    "run": {
      "stream": true
    },
    "version": {
      "message": "chore: publish ModelHub packages",
      "conventionalCommits": true,
      "changelog": false,
      "exact": true,
      "includeMergedTags": true,
      "private": false
    },
    "publish": {
      "npmClient": "npm",
      "registry": "https://registry.npmjs.org/"
    }
  },
  "ignoreChanges": ["**/*.md", "**/*.spec.ts", "**/*.spec.tsx"]
}
